{
   "compilerOptions": {
      "allowJs": true,
      "declaration": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "incremental": true,
      "lib": [ "ES2015", "ES2016", "ES2017", "ES2018", "ES2019", "ES2020" ],
      "module": "ES2020",
      "removeComments": true,
      "rootDir": "./",
      "strict": true,
      "skipLibCheck": true,
      "target": "es2020",
      "typeRoots": [ "dict" ],
      "watch": true
   },
   "exclude": [ "*index*" ]
}
